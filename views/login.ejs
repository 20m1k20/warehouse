<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet"
    integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
<link rel="stylesheet" href="css/css.css">
<script src="https://code.jquery.com/jquery-3.6.4.min.js"></script>
<title>Login</title>
<div id="login">
    <div>
        <h1>The Wearhouse</h1>
    </div>
    <form>
        <div>
            <div class="mb-3">
                <label for="exampleInputEmail1" class="form-label">Email address</label>
                <input style="background-color: rgb(210, 243, 210);" type="email" class="form-control"
                    id="exampleInputEmail1" aria-describedby="emailHelp">
                <div id="emailHelp" class="form-text">We'll never share your email with anyone else.</div>
            </div>
            <div class="mb-3">
                <label for="exampleInputPassword1" class="form-label">Password</label>
                <input style="background-color: rgb(210, 243, 210);" type="password" class="form-control"
                    id="exampleInputPassword1">
            </div>
            <button id="btn" type="submit" class="btn btn-primary">Log in</button>
        </div>
    </form>


    <h1 id="registrationbtn"><button id="btn1" type="submit" class="btn btn-primary">New user</button></h1>

</div>


<h2>
    <%= text %>
</h2>



